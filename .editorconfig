# editorconfig.org

root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false
indent_size = 2

[{package.json,.gitlab-ci.yml,.lando.yml,compose.yaml,compose.*.yaml}]
indent_size = 2
